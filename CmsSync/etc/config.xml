<?xml version="1.0"?>
<config>
    <modules>
        <CalinDiacon_CmsSync>
            <version>0.1.0</version>
        </CalinDiacon_CmsSync>
    </modules>
    <global>
        <models>
            <cmssync>
                <class>CalinDiacon_CmsSync_Model</class>
            </cmssync>
        </models>
        <blocks>
            <cmssync>
                <class>CalinDiacon_CmsSync_Block</class>
            </cmssync>
            <adminhtml>
                <rewrite>
                    <cms_block_edit>CalinDiacon_CmsSync_Block_Adminhtml_Cms_Block_Edit</cms_block_edit>
                    <cms_page_edit>CalinDiacon_CmsSync_Block_Adminhtml_Cms_Page_Edit</cms_page_edit>
                </rewrite>
            </adminhtml>
        </blocks>
        <helpers>
            <cmssync>
                <class>CalinDiacon_CmsSync_Helper</class>
            </cmssync>
        </helpers>
        <resources>
            <cmssync_setup>
                <setup>
                    <module>CalinDiacon_CmsSync</module>
                </setup>
            </cmssync_setup>
        </resources>
    </global>
    <admin>
        <routers>
            <cmssync>
                <use>admin</use>
                <args>
                    <modules>
                        <calindiacon_cmssync before="Mage_Adminhtml">CalinDiacon_CmsSync_Adminhtml</calindiacon_cmssync>
                    </modules>
                    <module>CalinDiacon_CmsSync_Adminhtml</module>
                    <frontName>cmssync</frontName>
                </args>
            </cmssync>
        </routers>
    </admin>
</config>